apiVersion: apps/v1

kind: Deployment

metadata:

  name: my-deployment

spec:

  replicas: 4

  strategy:

    type: Recreate

  selector:

    matchLabels:

      app: my-app

  template:

    metadata:

      labels:

        app: my-app

    spec:

      containers:

        - name: nginx-cont

          image: nginx









